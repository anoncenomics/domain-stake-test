{
  "name": "autonomys-epoch-reader",
  "version": "1.0.0",
  "type": "module",
  "description": "Auto EVM epoch reader for analyzing domain staking and rewards",
  "main": "auto-evm-epoch-reader.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "epoch": "node auto-evm-epoch-reader.js",
    "backfill": "node scripts/backfill-epochs.mjs --ws wss://rpc.mainnet.subspace.foundation/ws --domain 0 --from 0 --to current --append",
    "backfill:storage-fees": "node scripts/backfill-epochs-with-storage-fees.mjs --ws wss://rpc.mainnet.subspace.foundation/ws --domain 0 --from 0 --to current --append",
    "backfill:comprehensive": "node scripts/backfill-comprehensive-metrics.mjs --ws wss://rpc.anoncenomics.com/ws --domain 0 --from 0 --to current --append",
    "backfill:optimized": "node scripts/optimized-comprehensive-backfill.mjs --ws wss://rpc.anoncenomics.com/ws --domain 0 --from 0 --to current --concurrency 5 --batch-size 10",
    "backfill:fast": "node scripts/backfill-comprehensive-metrics.mjs --ws ws://192.168.0.99:9944 --domain 0 --db --concurrency 10 --batch-size 20 --resume",
    "backfill:local": "node scripts/backfill-comprehensive-metrics.mjs --ws ws://192.168.0.99:9944 --domain 0 --db --concurrency 5 --batch-size 10",
    "backfill:postgres": "node scripts/backfill-comprehensive-metrics-postgres.mjs --domain 0 --concurrency 10 --batch-size 20",
    "backfill:postgres:resume": "node scripts/backfill-comprehensive-metrics-postgres.mjs --domain 0 --concurrency 10 --batch-size 20 --resume",
    "backfill:postgres:validate": "node scripts/backfill-comprehensive-metrics-postgres.mjs --domain 0 --concurrency 10 --batch-size 20 --validate",
    "export:db": "node scripts/export-db-to-json.mjs",
    "benchmark": "node scripts/benchmark-backfill.mjs --from 0 --to 5",
    "profile": "node scripts/profile-bottlenecks.mjs --epoch 0 --iterations 3",
    "events:list": "node scripts/list-domain-events.mjs",
    "live:transfers": "node scripts/live-operator-transfers.mjs",
    "scan:transfers": "node scripts/scan-operator-transfers.mjs",
    "augment:shareprices": "node scripts/augment-share-prices.mjs",
    "cleanup:data": "node scripts/cleanup-data.mjs",
    "cleanup:data:dry": "node scripts/cleanup-data.mjs --dry-run",
    "cleanup:data:backup": "node scripts/cleanup-data.mjs --backup",
    "db:migrate": "node scripts/migrate-database.mjs",
    "db:migrate:dry": "node scripts/migrate-database.mjs --dry-run",
    "db:migrate:backup": "node scripts/migrate-database.mjs --backup",
    "db:migrate:postgres": "node scripts/migrate-sqlite-to-postgres.mjs --validate",
    "db:migrate:postgres:dry": "node scripts/migrate-sqlite-to-postgres.mjs --dry-run",
    "db:migrate:postgres:range": "node scripts/migrate-sqlite-to-postgres.mjs --from 0 --to 1000 --validate",
    "setup:postgres": "node scripts/setup-postgres.mjs",
    "setup:postgres:dry": "node scripts/setup-postgres.mjs --dry-run"
  },
  "dependencies": {
    "@autonomys/auto-drive": "^1.5.14",
    "@autonomys/auto-utils": "^1.5.14",
    "@polkadot/api": "^16.4.5",
    "@vercel/analytics": "^1.5.0",
    "next": "^14.2.5",
    "pg": "^8.16.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^2.12.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "polkadot",
    "autonomys",
    "blockchain",
    "staking",
    "epoch"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@types/react": "19.1.11",
    "@types/react-dom": "^19.1.7",
    "typescript": "^5.9.2"
  }
}
